{% extends 'base.html.twig' %}

{% block title %}Team{% endblock %}

{% block body %}
    <h1>Team</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ team.id }}</td>
            </tr>
            <tr>
                <th>Pays</th>
                <td>{{ team.pays }}</td>
            </tr>
            <tr>
                <th>Region</th>
                <td>{{ team.region }}</td>
            </tr>
            <tr>
                <th>Ville</th>
                <td>{{ team.ville }}</td>
            </tr>
            <tr>
                <th>Code_postal</th>
                <td>{{ team.codePostal }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('team_index') }}">back to list</a>

    <a href="{{ path('team_edit', {'id': team.id}) }}">edit</a>

    {{ include('team/_delete_form.html.twig') }}
